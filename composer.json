{
    "type": "project",
    "license": "proprietary",
    "name": "damienharper/auditor-bundle-demo",
    "description": "damienharper/auditor-bundle demo application.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "damienharper/auditor": "dev-master",
        "damienharper/auditor-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "gedmo/doctrine-extensions": "^3.0",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/runtime": "*",
        "symfony/stopwatch": "*",
        "symfony/twig-bundle": "*",
        "symfony/web-profiler-bundle": "*",
        "symfony/yaml": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4"
    },
    "repositories": [
        {
            "type" : "path",
            "url" : "/Users/damienharper/Projects/personal/auditor"
        },
        {
            "type" : "path",
            "url" : "/Users/damienharper/Projects/personal/auditor-bundle"
        }
    ]
}
